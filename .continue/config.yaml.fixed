schema: v1

env:
  OPENAI_API_KEY: ${secrets.OPENAI_API_KEY}
  OPENAI_PROJECT: ${secrets.OPENAI_PROJECT}   # optional

models:
  - name: openai/gpt-5
    provider: openai
    model: gpt-5
    apiKey: ${env.OPENAI_API_KEY}
    project: ${env.OPENAI_PROJECT}            # optional

  - name: openai/gpt-4
    provider: openai
    model: gpt-4o
    apiKey: ${env.OPENAI_API_KEY}
    project: ${env.OPENAI_PROJECT}            # optional